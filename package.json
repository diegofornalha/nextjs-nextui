{
  "name": "meu-projeto",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=4.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-deps": "yarn outdated",
    "check-security": "yarn audit",
    "update-safe": "yarn upgrade-interactive --latest",
    "update-core": "yarn upgrade next react react-dom",
    "update-ui": "yarn upgrade @nextui-org/react @nextui-org/system @nextui-org/theme",
    "test-deps": "yarn install --check-files",
    "validate": "yarn check-deps && yarn check-security && yarn lint",
    "deps:check": "yarn dlx @yarnpkg/doctor",
    "deps:update": "yarn upgrade-interactive",
    "deps:clean": "yarn dedupe",
    "deps:why": "yarn why",
    "analyze": "ANALYZE=true yarn build"
  },
  "dependencies": {
    "@nextui-org/react": "2.2.9",
    "@nextui-org/system": "2.0.15",
    "@nextui-org/theme": "2.1.17",
    "framer-motion": "10.18.0",
    "next": "14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "15.1.0",
    "@types/node": "20.10.6",
    "@types/react": "18.2.46",
    "@types/react-dom": "18.2.18",
    "@types/tailwindcss": "3.1.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "postcss": "8.4.32",
    "tailwind-variants": "0.3.0",
    "tailwindcss": "3.4.0",
    "typescript": "5.3.3"
  },
  "packageManager": "yarn@4.5.3",
  "resolutions": {
    "@nextui-org/react": "2.2.9",
    "@nextui-org/system": "2.0.15",
    "@nextui-org/theme": "2.1.17",
    "next": "14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "peerDependencies": {
    "react": "^18.0.0"
  }
}
